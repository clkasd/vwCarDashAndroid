        android:layout_width="200dp"
        android:layout_height="wrap_content"
        fancy:fb_defaultColor="#2E64FE"
        fancy:fb_focusColor="#9bd823"
        fancy:fb_radius="30dp"
        fancy:fb_text="9.Kanal"
        android:id="@+id/ninethchannel"
        android:layout_marginLeft="680dp"
        android:layout_marginTop="420dp"
        fancy:fb_textColor="#FFFFFF" />
    <mehdi.sakout.fancybuttons.FancyButton
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        fancy:fb_defaultColor="#2E64FE"
        fancy:fb_focusColor="#9bd823"
        fancy:fb_radius="30dp"
        android:id="@+id/tenthchannel"
        fancy:fb_text="10.Kanal"
        android:layout_marginLeft="580dp"
        android:layout_marginTop="510dp"
        fancy:fb_textColor="#FFFFFF" />
    <org.adw.library.widgets.discreteseekbar.DiscreteSeekBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/radioSeek"
        android:layout_below="@id/currentFreq"
        app:dsb_min="8900"
        app:dsb_max="10700"
        />

</RelativeLayout>